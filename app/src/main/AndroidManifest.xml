<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.priansyah.demo1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
                  android:label="@string/inventory">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DetailItemActivity"
                  android:label="@string/detail_item">
        </activity>
        <activity android:name=".TambahItemActivity"
            android:label="@string/tambah_item">
        </activity>
        <activity android:name=".DetailKategoriActivity"
            android:label="@string/detail_kategori">
        </activity>
        <activity android:name=".TambahKategoriActivity"
            android:label="@string/tambah_kategori">
        </activity>
        <activity android:name=".DetailSupplierActivity"
            android:label="@string/detail_supplier">
        </activity>
        <activity android:name=".TambahSupplierActivity"
            android:label="@string/tambah_supplier">
        </activity>
        <activity android:name=".InputProductName"
            android:label="@string/input_product_name">
        </activity>
        <activity android:name=".EditItemActivity"
            android:label="@string/edit_item">
        </activity>
        <activity android:name=".EditKategoriActivity"
            android:label="@string/edit_kategori">
        </activity>
        <activity android:name=".EditSupplierActivity"
            android:label="@string/edit_supplier">
        </activity>
        <activity android:name=".DiskonActivity"
            android:label="@string/diskon">
        </activity>
        <activity android:name=".KonfirmasiActivity"
            android:label="@string/konfirmasi">
        </activity>
        <activity android:name=".ReceiptActivity"
            android:label="@string/receipt">
        </activity>
        <activity android:name=".TransaksiActivity"
            android:label="@string/transaksi">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable" />
        </activity>
        <activity android:name=".InventoryActivity"
            android:label="@string/inventory">
        </activity>
        <activity android:name=".KirimReceiptActivity"
            android:label="@string/kirim_receipt">
        </activity>
        <activity android:name=".PilihItemTransaksiActivity"
            android:label="@string/pilih_item_transaksi">
        </activity>
        <activity android:name=".DetailTransaksiActivity"
            android:label="@string/detail_transaksi">
        </activity>
        <activity android:name=".RefundActivity"
            android:label="@string/refund">
        </activity>
        <activity android:name=".LoginActivity"
            android:label="@string/refund">
        </activity>
        <activity android:name=".RegisterActivity"
            android:label="@string/refund">
        </activity>
        <activity android:name=".HomeActivity"
            android:label="@string/refund">
        </activity>
    </application>

</manifest>
